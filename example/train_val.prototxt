# l2 scale
layer {
  name: "norm"
  type: "NormL2"
  bottom: "fc5"
  top: "norm"
}
layer {
  name: "scale"
  type: "Scale"
  bottom: "norm"
  top: "scale"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  scale_param {
    filler {
      value: 32.0
    }
    bias_term: false
  }
}
# sphereface
layer {
  name: "loss3/casia_res20net/label000"
  type: "SmoothMarginInnerProduct"
  bottom: "scale"
  bottom: "label000"
  top: "loss3/casia_res20net/label000"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  smooth_margin_inner_product_param {
    num_output: 5000
    type: QUADRUPLE
    weight_filler {
      type: "xavier"
    }
    base: 5.0
    gamma: 0.12
    power: 1
    lambda_min: 5.0
    iteration: 0
  }
}
# LSR loss
layer {
  name: "loss3/loss/label000"
  type: "LSROLoss"
  bottom: "loss3/casia_res20net/label000"
  bottom: "label000"
  top: "loss3/loss/label000"
  loss_weight: 1.0
}
# DSA loss
layer {
  name: "dsa_loss"
  type: "DSALoss"
  #bottom: "norm"
  bottom: "fc5"
  bottom: "label000"
  top: "dsa_loss"
  param {
    lr_mult: 1
    decay_mult: 0
  }
  dsa_loss_param {
    faceid_num: 5000
    seqid_num: 32996
    center_filler {
      type: "xavier"
    }
    lambda: 0.5
    dropout_ratio: 0.99
    gamma: 1.0
    alpha: 2.0
    beta: 1.0
    #use_normalize: true
    use_normalize: false
    scale: 1.0
  }
  loss_weight: 0.04
}
