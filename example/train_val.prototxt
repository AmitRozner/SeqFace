name: "face_res20net"
# identity dataset
# the label of identity dataset range from 0 to faceid_num - 1
layer {
  name: "data_part1"
  type: "ImageData"
  top: "data_part1"
  top: "label000_part1"
  transform_param {
    crop_size: 128
    scale: 0.0078125
    mirror: true
    mean_file: "@model_dir/mean.binaryproto"
  }
  image_data_param {
    source: "@data_dir/image_train_part1.txt"
    shuffle: true
    batch_size: 32
  }
}
# sequence dataset
# the label of sequence dataset range from -1 to -seqid_num
layer {
  name: "data_part2"
  type: "ImageData"
  top: "data_part2"
  top: "label000_part2"
  transform_param {
    crop_size: 128
    scale: 0.0078125
    mirror: true
    mean_file: "@model_dir/mean.binaryproto"
  }
  image_data_param {
    source: "@data_dir/image_train_part2_seq.txt"
    shuffle: true
    batch_size: 32
  }
}
# concat two dataset
layer {
  name: "data"
  type: "Concat"
  bottom: "data_part1"
  bottom: "data_part2"
  top: "data"
  concat_param {
    axis: 0
  }
}
layer {
  name: "label000"
  type: "Concat"
  bottom: "label000_part1"
  bottom: "label000_part2"
  top: "label000"
  concat_param {
    axis: 0
  }
}
# basic network
layer {
  name: "conv1_1"
  type: "Convolution"
  bottom: "data"
  top: "conv1_1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 2
    pad: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_1"
  type: "PReLU"
  bottom: "conv1_1"
  top: "conv1_1"
}
layer {
  name: "conv1_2"
  type: "Convolution"
  bottom: "conv1_1"
  top: "conv1_2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_2"
  type: "PReLU"
  bottom: "conv1_2"
  top: "conv1_2"
}
layer {
  name: "conv1_3"
  type: "Convolution"
  bottom: "conv1_2"
  top: "conv1_3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 64
    kernel_size: 3
    stride: 1
    pad: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_3"
  type: "PReLU"
  bottom: "conv1_3"
  top: "conv1_3"
}
layer {
  name: "res1_3"
  type: "Eltwise"
  bottom: "conv1_1"
  bottom: "conv1_3"
  top: "res1_3"
  eltwise_param { 
    operation: 1
  }
}
